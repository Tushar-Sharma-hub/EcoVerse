{
  "name": "ecoverse-dashboard",
  "version": "1.0.0",
  "description": "Environmental monitoring platform with 3D Earth visualization and AI-powered insights",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "start": "concurrently \"npm run backend:start\" \"npm run frontend:build\"",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "frontend:dev": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/ecoverse-dashboard.git"
  },
  "keywords": [
    "environmental-monitoring",
    "climate-data",
    "3d-visualization",
    "ai-powered",
    "urban-planning",
    "sustainability",
    "maharashtra",
    "satellite-data"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/ecoverse-dashboard/issues"
  },
  "homepage": "https://github.com/your-username/ecoverse-dashboard#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5"
  }
}
